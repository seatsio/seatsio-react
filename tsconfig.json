    "lib": ["dom", "es2015"],
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["@seatsio/seatsio-types", "jest"]
  },
  "include": [
      "./src/main",
      "./src/types"
  ],
